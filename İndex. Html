                <div class="location-card" onclick="showLocationMedicines('mutfak')">
                    <div class="location-icon">üç≥</div>
                    <div class="location-name">Mutfak</div>
                </div>
                <div class="location-card" onclick="showLocationMedicines('yatak')">
                    <div class="location-icon">üõèÔ∏è</div>
                    <div class="location-name">Yatak Odasƒ±</div>
                </div>
                <div class="location-card" onclick="showLocationMedicines('saglik')">
                    <div class="location-icon">ü©∫</div>
                    <div class="location-name">Saƒülƒ±k √áantasƒ±</div>
                </div>
            </div>
            <div id="locationMedicineList"></div>
        </div>
        
