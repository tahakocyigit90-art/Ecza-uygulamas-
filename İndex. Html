                        <div class="location-name">Banyo Dolabƒ±</div>
                    </div>
                    <div class="location-card" onclick="selectLocation('mutfak')">
                        <div class="location-icon">üç≥</div>
                        <div class="location-name">Mutfak</div>
                    </div>
                    <div class="location-card" onclick="selectLocation('yatak')">
                        <div class="location-icon">üõèÔ∏è</div>
                        <div class="location-name">Yatak Odasƒ±</div>
                    </div>
                    <div class="location-card" onclick="selectLocation('saglik')">
                        <div class="location-icon">ü©∫</div>
                        <div class="location-name">Saƒülƒ±k √áantasƒ±</div>
                    </div>
                </div>
                
                <div id="shelfSection" style="display:none;margin-top:10px">
                    <label class="form-label">Raf/√áekmece No</label>
                    <div class="shelf-grid" id="shelfGrid">
                        <button class="shelf-btn" onclick="selectShelf(1)">1</button>
                        <button class="shelf-btn" onclick="selectShelf(2)">2</button>
                        <button class="shelf-btn" onclick="selectShelf(3)">3</button>
                        <button class="shelf-btn" onclick="selectShelf(4)">4</button>
                        <button class="shelf-btn" onclick="selectShelf(5)">5</button>
                        <button class="shelf-btn" onclick="selectShelf(6)">6</button>
                        <button class="shelf-btn" onclick="selectShelf(7)">7</button>
