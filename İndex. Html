    <script>
        // Veri y√∂netimi
        let medicines = JSON.parse(localStorage.getItem('evimdeIlac')) || [];
        let currentMedicine = {photo:null,category:null,location:null,shelf:null,alarmDays:7};
        let currentFilter = 'all';
        let selectedMedicineId = null;
        
        const categories = {
            agri:{name:'Aƒürƒ± Kesici',color:'#e74c3c',icon:'üíä'},
            antibiyotik:{name:'Antibiyotik',color:'#9b59b6',icon:'ü¶†'},
            vitamin:{name:'Vitamin',color:'#27ae60',icon:'üçä'},
            seker:{name:'≈ûeker/Tansiyon',color:'#f39c12',icon:'ü©∏'},
            kalp:{name:'Kalp',color:'#e91e63',icon:'‚ù§Ô∏è'},
            mide:{name:'Mide/Baƒüƒ±rsak',color:'#00bcd4',icon:'ü§¢'},
            alerji:{name:'Alerji',color:'#795548',icon:'ü§ß'},
            goz:{name:'G√∂z/Kulak',color:'#3f51b5',icon:'üëÅÔ∏è'},
            cilt:{name:'Cilt/Merhem',color:'#ff9800',icon:'üß¥'},
            diger:{name:'Diƒüer',color:'#607d8b',icon:'üíâ'}
        };
        
        const locations = {
            banyo:{name:'Banyo Dolabƒ±',icon:'üöø'},
            mutfak:{name:'Mutfak',icon:'üç≥'},
            yatak:{name:'Yatak Odasƒ±',icon:'üõèÔ∏è'},
            saglik:{name:'Saƒülƒ±k √áantasƒ±',icon:'ü©∫'}
        };
        
        // Ekran y√∂netimi
        function showScreen(screenId) {
            document.querySelectorAll('.screen').forEach(s => s.classList.remove('active'));
            document.getElementById(screenId).classList.add('active');
            document.getElementById('backBtn').style.display = screenId === 'mainScreen' ? 'none' : 'flex';
            updateStats();
        }
        
        function goBack() {
            showScreen('mainScreen');
            resetForm();
        }
        
        // Ana men√º fonksiyonlarƒ±
        function showAddMedicine() {
            showScreen('addScreen');
            resetForm();
        }
        
        function showMedicineList() {
            showScreen('listScreen');
            renderMedicineList();
        }
        
        function showLocationView() {
            showScreen('locationScreen');
        }
        
        function showExpiring() {
            showScreen('expiryScreen');
            renderExpiryList();
        }
        
        function showEmergency() {
            document.getElementById('emergencyModal').classList.add('active');
        }
        
        function closeEmergencyModal() {
            document.getElementById('emergencyModal').classList.remove('active');
        }
        
        // Form i≈ülemleri
        function resetForm() {
            currentMedicine = {photo:null,category:null,location:null,shelf:null,alarmDays:7};
            document.getElementById('medicineName').value = '';
            document.getElementById('activeIngredient').value = '';
            document.getElementById('medicineDose').value = '';
            document.getElementById('usageType').value = 'tablet';
            document.getElementById('dailyDose').textContent = '3';
            document.getElementById('expiryDate').value = '';
            document.getElementById('medicineNotes').value = '';
            document.getElementById('photoPreview').innerHTML = '<div class="photo-placeholder">üì∑</div>';
            document.querySelectorAll('.category-card').forEach(c => c.classList.remove('selected'));
            document.querySelectorAll('.location-card').forEach(c => c.classList.remove('selected'));
            document.querySelectorAll('.shelf-btn').forEach(c => c.classList.remove('selected'));
            document.querySelectorAll('.alarm-chip').forEach((c,i) => c.classList.toggle('selected', i===0));
            document.getElementById('shelfSection').style.display = 'none';
        }
        
        function handlePhoto(input) {
            if(input.files && input.files[0]) {
                const reader = new FileReader();
                reader.onload = function(e) {
                    currentMedicine.photo = e.target.result;
                    document.getElementById('photoPreview').innerHTML = `<img src="${e.target.result}">`;
                };
                reader.readAsDataURL(input.files[0]);
            }
        }
        
        function selectCategory(cat) {
            currentMedicine.category = cat;
            document.querySelectorAll('.category-card').forEach(c => c.classList.remove('selected'));
            event.currentTarget.classList.add('selected');
        }
        
        function selectLocation(loc) {
            currentMedicine.location = loc;
            document.querySelectorAll('#locationGrid .location-card').forEach(c => c.classList.remove('selected'));
            event.currentTarget.classList.add('selected');
            document.getElementById('shelfSection').style.display = 'block';
        }
        
        function selectShelf(num) {
            currentMedicine.shelf = num;
            document.querySelectorAll('.shelf-btn').forEach(c => c.classList.remove('selected'));
            event.target.classList.add('selected');
        }
        
        function changeDose(change) {
            const display = document.getElementById('dailyDose');
            let current = parseInt(display.textContent);
            current += change;
            if(current < 1) current = 1;
            if(current > 10) current = 10;
            display.textContent = current;
        }
        
        function selectAlarm(element, days) {
            currentMedicine.alarmDays = days;
            document.querySelectorAll('.alarm-chip').forEach(c => c.classList.remove('selected'));
            element.classList.add('selected');
        }
        
        function saveMedicine() {
            const name = document.getElementById('medicineName').value.trim();
            const ingredient = document.getElementById('activeIngredient').value.trim();
            const dose = document.getElementById('medicineDose').value.trim();
            const usageType = document.getElementById('usageType').value;
            const dailyDose = document.getElementById('dailyDose').textContent;
            const expiry = document.getElementById('expiryDate').value;
            const notes = document.getElementById('medicineNotes').value.trim();
            
            if(!name) { showNotification('ƒ∞la√ß adƒ± girmelisiniz!', 'error'); return; }
            if(!currentMedicine.category) { showNotification('Kategori se√ßmelisiniz!', 'error'); return; }
            if(!expiry) { showNotification('Son kullanma tarihi girmelisiniz!', 'error'); return; }
            if(!currentMedicine.location) { showNotification('Konum se√ßmelisiniz!', 'error'); return; }
            
            const medicine = {
                id: Date.now(),
                name: name,
                ingredient: ingredient,
                category: currentMedicine.category,
                dose: dose,
                usageType: usageType,
                dailyDose: dailyDose,
                expiry: expiry,
                alarmDays: currentMedicine.alarmDays,
                location: currentMedicine.location,
                shelf: currentMedicine.shelf,
                notes: notes,
                photo: currentMedicine.photo,
                added: new Date().toLocaleDateString('tr-TR')
            };
            
            medicines.push(medicine);
            localStorage.setItem('evimdeIlac', JSON.stringify(medicines));
            
            showNotification('ƒ∞la√ß ba≈üarƒ±yla kaydedildi! ‚úÖ');
            goBack();
        }
        
        // ƒ∞la√ß listesi
        function renderMedicineList(filter = 'all', search = '') {
            const list = document.getElementById('medicineList');
            list.innerHTML = '';
            
            let filtered = medicines;
            if(filter !== 'all') filtered = filtered.filter(m => m.category === filter);
            if(search) filtered = filtered.filter(m => 
                m.name.toLowerCase().includes(search.toLowerCase()) || 
                (m.ingredient && m.ingredient.toLowerCase().includes(search.toLowerCase()))
            );
            
            if(filtered.length === 0) {
                list.innerHTML = `
                    <div class="empty-state">
                        <div class="empty-icon">üíä</div>
                        <div class="empty-text">Hen√ºz ila√ß eklenmemi≈ü</div>
                        <button class="btn btn-primary" onclick="showAddMedicine()">ƒ∞la√ß Ekle</button>
                    </div>
                `;
                return;
            }
            
            filtered.forEach(medicine => {
                const cat = categories[medicine.category];
                const loc = locations[medicine.location];
                const expiryClass = getExpiryClass(medicine.expiry, medicine.alarmDays);
                const expiryText = formatDate(medicine.expiry);
                
                const card = document.createElement('div');
                card.className = 'medicine-card';
                card.style.borderLeftColor = cat.color;
                card.onclick = () => showMedicineDetail(medicine.id);
                card.innerHTML = `
                    <div class="medicine-image">
                        ${medicine.photo ? `<img src="${medicine.photo}">` : '<div class="no-photo">üíä</div>'}
                    </div>
                    <div class="medicine-info">
                        <div class="medicine-name">${cat.icon} ${medicine.name}</div>
                        <div class="medicine-meta">
                            <span class="medicine-dose">${medicine.dose || 'Doz belirtilmemi≈ü'}</span>
                            <span class="medicine-location">üìç ${loc.name}${medicine.shelf ? ' - Raf ' + medicine.shelf : ''}</span>
                        </div>
                        <div class="medicine-expiry ${expiryClass}">üìÖ ${expiryText}</div>
                    </div>
                    <button class="medicine-delete" onclick="event.stopPropagation(); deleteMedicine(${medicine.id})">üóëÔ∏è</button>
                `;
                list.appendChild(card);
            });
        }
        
        function searchMedicines() {
            const search = document.getElementById('searchInput').value;
            renderMedicineList(currentFilter, search);
        }
        
        function filterMedicines(cat) {
            currentFilter = cat;
            document.querySelectorAll('.filter-chip').forEach(c => c.classList.remove('active'));
            event.target.classList.add('active');
            renderMedicineList(cat, document.getElementById('searchInput').value);
        }
        
        // Konum g√∂r√ºn√ºm√º
        function showLocationMedicines(loc) {
            const locMedicines = medicines.filter(m => m.location === loc);
            const container = document.getElementById('locationMedicineList');
            const locInfo = locations[loc];
            
            if(locMedicines.length === 0) {
                container.innerHTML = `
                    <div class="empty-state">
                        <div class="empty-icon">üì≠</div>
                        <div class="empty-text">${locInfo.name} bo≈ü</div>
                    </div>
                `;
                return;
            }
            
            // Raflara g√∂re grupla
            const shelves = {};
            locMedicines.forEach(m => {
                const shelf = m.shelf || 'Belirtilmemi≈ü';
                if(!shelves[shelf]) shelves[shelf] = [];
                shelves[shelf].push(m);
            });
            
            let html = `<h4 style="margin-bottom:15px;color:#4a5568">${locInfo.icon} ${locInfo.name} - ${locMedicines.length} ila√ß</h4>`;
            
            Object.keys(shelves).sort().forEach(shelf => {
                html += `<div style="background:#f7fafc;border-radius:10px;padding:15px;margin-bottom:15px">`;
                html += `<h5 style="margin-bottom:10px;color:#e74c3c">üìç Raf ${shelf}</h5>`;
                shelves[shelf].forEach(m => {
                    const cat = categories[m.category];
                    const expiryClass = getExpiryClass(m.expiry, m.alarmDays);
                    html += `
                        <div style="display:flex;align-items:center;gap:10px;padding:10px;background:white;border-radius:8px;margin-bottom:8px;cursor:pointer;border-left:4px solid ${cat.color}" onclick="showMedicineDetail(${m.id})">
                            <div style="font-size:1.5em">${cat.icon}</div>
                            <div style="flex:1">
                                <div style="font-weight:600">${m.name}</div>
                                <div style="font-size:0.8em;color:#718096">${m.dose || 'Doz yok'}</div>
                                <div style="font-size:0.75em;margin-top:3px;padding:2px 6px;border-radius:6px;display:inline-block" class="${expiryClass}">üìÖ ${formatDate(m.expiry)}</div>
                            </div>
                        </div>
                    `;
                });
                html += `</div>`;
            });
            
            container.innerHTML = html;
        }
        
        // SKT Uyarƒ±larƒ±
        function renderExpiryList() {
            const list = document.getElementById('expiryList');
            const now = new Date();
            const warnings = [];
            
            medicines.forEach(m => {
                if(!m.expiry) return;
                const expiry = new Date(m.expiry);
                const alarmDate = new Date(expiry);
                alarmDate.setDate(alarmDate.getDate() - (m.alarmDays || 7));
                
                const diff = Math.ceil((expiry - now) / (1000 * 60 * 60 * 24));
                
                if(diff < 0) warnings.push({...m, status:'expired', days:diff});
                else if(diff <= (m.alarmDays || 7)) warnings.push({...m, status:'warning', days:diff});
            });
            
            if(warnings.length === 0) {
                list.innerHTML = `
                    <div class="empty-state">
                        <div class="empty-icon">‚úÖ</div>
                        <div class="empty-text">T√ºm ila√ßlar g√ºvende!</div>
                        <p style="font-size:0.9em;margin-top:10px">Son kullanma tarihi yakla≈üan ila√ß yok.</p>
                    </div>
                `;
                return;
            }
            
            warnings.sort((a,b) => a.days - b.days);
            
            list.innerHTML = warnings.map(m => {
                const cat = categories[m.category];
                const isExpired = m.status === 'expired';
                return `
                    <div style="background:${isExpired ? '#fed7d7' : '#fefcbf'};border-radius:12px;padding:15px;margin-bottom:10px;display:flex;align-items:center;gap:12px;cursor:pointer" onclick="showMedicineDetail(${m.id})">
                        <div style="font-size:2em">${isExpired ? '‚ùå' : '‚ö†Ô∏è'}</div>
                        <div style="flex:1">
                            <div style="font-weight:700;color:${isExpired ? '#742a2a' : '#744210'}">${cat.icon} ${m.name}</div>
                            <div style="font-size:0.85em;margin-top:3px">
                                ${isExpired ? `‚è∞ ${Math.abs(m.days)} g√ºn √∂nce doldu!` : `‚è∞ ${m.days} g√ºn kaldƒ±`}
                            </div>
                            <div style="font-size:0.8em;color:#718096;margin-top:3px">
                                üìç ${locations[m.location].name}${m.shelf ? ' - Raf ' + m.shelf : ''}
                            </div>
                        </div>
                    </div>
                `;
            }).join('');
        }
        
        // ƒ∞la√ß detay
        function showMedicineDetail(id) {
            const medicine = medicines.find(m => m.id === id);
            if(!medicine) return;
            
            selectedMedicineId = id;
            const cat = categories[medicine.category];
            const loc = locations[medicine.location];
            
            document.getElementById('modalMedicineName').textContent = medicine.name;
            document.getElementById('modalImage').innerHTML = medicine.photo ? 
                `<img src="${medicine.photo}">` : '<div style="color:#ccc;font-size:3em">üíä</div>';
            document.getElementById('modalIngredient').textContent = medicine.ingredient || 'Belirtilmemi≈ü';
            document.getElementById('modalCategory').textContent = `${cat.icon} ${cat.name}`;
            document.getElementById('modalDose').textContent = `${medicine.dose || 'Belirtilmemi≈ü'} - G√ºnde ${medicine.dailyDose} kez (${medicine.usageType})`;
            document.getElementById('modalLocation').textContent = `${loc.icon} ${loc.name}${medicine.shelf ? ' - Raf ' + medicine.shelf : ''}`;
            document.getElementById('modalExpiry').textContent = formatDate(medicine.expiry);
            
            // Uyarƒ± kontrol√º
            const now = new Date();
            const expiry = new Date(medicine.expiry);
            const diff = Math.ceil((expiry - now) / (1000 * 60 * 60 * 24));
            const alertDiv = document.getElementById('modalAlert');
            
            if(diff < 0) {
                alertDiv.textContent = '‚ö†Ô∏è BU ƒ∞LACIN SON KULLANMA TARƒ∞Hƒ∞ GE√áMƒ∞≈û! KULLANMAYIN!';
                alertDiv.style.display = 'block';
            } else if(diff <= (medicine.alarmDays || 7)) {
                alertDiv.textContent = `‚ö†Ô∏è Dikkat: ${diff} g√ºn i√ßinde son kullanma tarihi dolacak!`;
                alertDiv.style.display = 'block';
            } else {
                alertDiv.style.display = 'none';
            }
            
            // Notlar
            if(medicine.notes) {
                document.getElementById('modalNotes').textContent = medicine.notes;
                document.getElementById('modalNotesSection').style.display = 'block';
            } else {
                document.getElementById('modalNotesSection').style.display = 'none';
            }
            
            document.getElementById('medicineModal').classList.add('active');
        }
        
        function closeModal() {
            document.getElementById('medicineModal').classList.remove('active');
            selectedMedicineId = null;
        }
        
        function deleteMedicine(id) {
            if(confirm('Bu ilacƒ± silmek istediƒüinize emin misiniz?')) {
                medicines = medicines.filter(m => m.id !== id);
                localStorage.setItem('evimdeIlac', JSON.stringify(medicines));
                renderMedicineList();
                updateStats();
                showNotification('ƒ∞la√ß silindi üóëÔ∏è');
            }
        }
        
        function deleteCurrentMedicine() {
            if(selectedMedicineId) {
                deleteMedicine(selectedMedicineId);
                closeModal();
            }
        }
        
        // Yardƒ±mcƒ± fonksiyonlar
        function getExpiryClass(expiry, alarmDays = 7) {
            if(!expiry) return '';
            const now = new Date();
            const exp = new Date(expiry);
            const diff = Math.ceil((exp - now) / (1000 * 60 * 60 * 24));
            
            if(diff < 0) return 'expiry-danger';
            if(diff <= alarmDays) return 'expiry-warning';
            return 'expiry-good';
        }
        
        function formatDate(dateStr) {
            if(!dateStr) return '-';
            const date = new Date(dateStr);
            return date.toLocaleDateString('tr-TR');
        }
        
        function updateStats() {
            document.getElementById('totalMedicines').textContent = `üíä ${medicines.length} ƒ∞la√ß`;
            
            const now = new Date();
            const warnings = medicines.filter(m => {
                if(!m.expiry) return false;
                const exp = new Date(m.expiry);
                const alarmDate = new Date(exp);
                alarmDate.setDate(alarmDate.getDate() - (m.alarmDays || 7));
                return now >= alarmDate;
            }).length;
            
            document.getElementById('expiryAlert').textContent = `‚ö†Ô∏è ${warnings} Uyarƒ±`;
        }
        
        function showNotification(message, type = 'success') {
            const notif = document.getElementById('notification');
            notif.textContent = message;
            notif.className = 'notification show ' + type;
            setTimeout(() => notif.classList.remove('show'), 3000);
        }
        
        // Ba≈ülangƒ±√ß
        updateStats();
    </script>
</body>
</html>'''

# Kaydet
with open('/mnt/kimi/output/evimde_ilac.html', 'w', encoding='utf-8') as f:
    f.write(html_content)

