                <textarea class="form-input" id="medicineNotes" placeholder="Doktorun √∂nerisi, yan etkiler, vb."></textarea>
            </div>
            
            <button class="btn btn-success" onclick="saveMedicine()">üíæ ƒ∞lacƒ± Kaydet</button>
            <button class="btn btn-secondary" onclick="goBack()">ƒ∞ptal</button>
        </div>
        
        <!-- ƒ∞la√ß Listesi -->
        <div class="screen" id="listScreen">
            <div class="search-box">
                <span class="search-icon">üîç</span>
                <input type="text" class="search-input" id="searchInput" placeholder="ƒ∞la√ß adƒ± veya etken madde ara..." oninput="searchMedicines()">
            </div>
            
            <div class="search-filters">
                <button class="filter-chip active" onclick="filterMedicines('all')">T√ºm√º</button>
                <button class="filter-chip" onclick="filterMedicines('agri')">üíä Aƒürƒ±</button>
                <button class="filter-chip" onclick="filterMedicines('antibiyotik')">ü¶† Antibiyotik</button>
                <button class="filter-chip" onclick="filterMedicines('vitamin')">üçä Vitamin</button>
                <button class="filter-chip" onclick="filterMedicines('seker')">ü©∏ ≈ûeker</button>
            </div>
            
            <div class="medicine-list" id="medicineList"></div>
        </div>
        
        <!-- Konuma G√∂re G√∂r√ºn√ºm -->
        <div class="screen" id="locationScreen">
            <h3 style="margin-bottom:15px;color:#2d3748">üìç Nerede?</h3>
            <div class="location-grid" style="margin-bottom:20px">
                <div class="location-card" onclick="showLocationMedicines('banyo')">
                    <div class="location-icon">üöø</div>
